{
  "name": "ironspread",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/main.js",
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.7",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron-builder": "^22.9.1",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "eslint-plugin-react": "^7.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.1",
    "wait-on": "^5.2.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .ts --ext tsx .",
    "check-types": "tsc",
    "dev:electron": "NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "NODE_ENV=development webpack serve --config webpack.react.config.js --mode development",
    "dev": "concurrently \"npm run dev:react \" \"wait-on http://localhost:4000 && npm run dev:electron\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@types/webpack-env": "^1.16.0",
    "css-loader": "^5.0.1",
    "electron": "^11.0.3",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "html-webpack-plugin": "^5.0.0-alpha.15",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "svg-url-loader": "^7.1.1",
    "ts-jest": "^26.4.4",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "roots": [
      "src/"
    ],
    "preset": "ts-jest",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webpttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(svg)$": "<rootDir>/__mocks__/svgrMock.ts",
      "\\.(css)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  }
}
